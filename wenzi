            "禫": { char: "禫", description: "⿰礻覃", pinyin: "dǎn", zhuyin: "ㄉㄢˇ", radical: "示", strokes: 15, meaning: { "說文解字": "無", "廣韻": "無", "康熙字典": "《廣韻》徒覽切，上聲，感韻。祭名。", "中華字海": "指古代除去喪服的祭祀。", "漢語大字典": "1. 祭名，除去喪服之祭。2. 喪禮。", "其他": "古代喪禮結束後，脫去喪服的祭祀儀式。" } },
        };
        
        /**
         * 函式：根據提供的資料建立一個詳細的字符卡片。
         * @param {object} characterData - 字符的資料物件。
         * @returns {HTMLElement} 建立好的卡片元素。
         */
        function createCharacterCard(characterData) {
            const characterCard = document.createElement('div');
            characterCard.className = 'character-card bg-white p-6 rounded-lg shadow-md text-left space-y-4';
            
            const titleContainer = document.createElement('div');
            titleContainer.className = 'text-center mb-4';
            
            const char = characterData.char;
            
            const characterDisplay = document.createElement('div');
            characterDisplay.className = 'character-display-container';

            // 處理特殊字元 '󸾯' 的圖片顯示，因為瀏覽器原生字體可能無法正確顯示
            if (char === '󸾯') {
                const characterText = document.createElement('h2');
                characterText.className = 'text-[80px] text-gray-800 font-zhengkai-unicode2';
                characterText.textContent = char;
                characterDisplay.appendChild(characterText);

                const characterImage = document.createElement('img');
                characterImage.src = "https://www.cns11643.gov.tw/char/kai/80/F8FAF.png";
                characterImage.alt = char;
                characterImage.className = 'character-image w-20 h-20';
                characterDisplay.appendChild(characterImage);
            } else {
                // 對於其他字符，同步顯示文字與圖片
                const characterText = document.createElement('h2');
                characterText.className = 'character-text text-gray-800';
                characterText.textContent = char;
                characterDisplay.appendChild(characterText);

                const characterImage = document.createElement('img');
                characterImage.src = generateCharacterImage(char, 80);
                characterImage.alt = char;
                characterImage.className = 'character-image w-20 h-20';
                characterDisplay.appendChild(characterImage);
            }

            titleContainer.appendChild(characterDisplay);
            characterCard.appendChild(titleContainer);

            // 建立資訊表格
            const infoTable = document.createElement('table');
            infoTable.className = 'w-full text-left border-collapse mb-4';
            const infoTableBody = document.createElement('tbody');
            infoTable.appendChild(infoTableBody);

            // 輔助函式：建立表格列
            const createRow = (header, content, className = '') => {
                const row = document.createElement('tr');
                row.className = 'border-b border-gray-200';
                row.innerHTML = `
                    <td class="py-1 px-2 text-gray-700 w-1/4 header-cell">${header}</td>
                    <td class="py-1 px-2 text-gray-700 ${className}">${content}</td>
                `;
                return row;
            };

            infoTableBody.appendChild(createRow('字符描述', characterData.description || '無'));
            // 處理有多個讀音的字
            if (characterData.tones) {
                const allZhuyin = characterData.tones.map(t => t.zhuyin).join('、');
                const allPinyin = characterData.tones.map(t => t.pinyin).join('、');
                infoTableBody.appendChild(createRow('注音', allZhuyin));
                infoTableBody.appendChild(createRow('拼音', allPinyin, 'pinyin'));
            } else {
                infoTableBody.appendChild(createRow('注音', characterData.zhuyin || '無'));
                infoTableBody.appendChild(createRow('拼音', characterData.pinyin || '無', 'pinyin'));
            }

            infoTableBody.appendChild(createRow('部首', characterData.radical || '無'));
            infoTableBody.appendChild(createRow('筆畫數', characterData.strokes || '無'));
            characterCard.appendChild(infoTable);
            
            // 處理釋義部分
            const meaningSection = document.createElement('div');
            meaningSection.className = 'p-4 rounded-lg';
            const meaningTitle = document.createElement('h3');
            meaningTitle.className = 'text-lg text-gray-700 mb-2 text-center';
            meaningTitle.textContent = '釋義';
            meaningSection.appendChild(meaningTitle);

            const meaningContainer = document.createElement('div');
            meaningContainer.className = 'divide-y divide-gray-200';
            
            if (characterData.tones) {
                // 如果有多個讀音，分別顯示釋義
                characterData.tones.forEach(tone => {
                    const toneSection = document.createElement('div');
                    toneSection.className = 'p-4 rounded-lg bg-gray-100 mt-4';
                    const toneHeader = document.createElement('h4');
                    toneHeader.className = 'text-md text-center text-gray-800 mb-2';
                    // 這裡的拼音也需要應用 Times New Roman
                    toneHeader.innerHTML = `讀音：${tone.zhuyin} (<span class="pinyin">${tone.pinyin}</span>)`;
                    toneSection.appendChild(toneHeader);
                    const toneMeaningContainer = document.createElement('div');
                    toneMeaningContainer.className = 'divide-y divide-gray-200';
                    const meaningSources = Object.keys(tone.meaning);
                    meaningSources.forEach(source => {
                        const rowDiv = document.createElement('div');
                        rowDiv.className = 'grid grid-cols-[120px_1fr] gap-x-2 py-1';
                        const sourceDiv = document.createElement('div');
                        sourceDiv.textContent = source;
                        rowDiv.appendChild(sourceDiv);
                        const meaningDiv = document.createElement('div');
                        meaningDiv.textContent = tone.meaning[source];
                        rowDiv.appendChild(meaningDiv);
                        toneMeaningContainer.appendChild(rowDiv);
                    });
                    toneSection.appendChild(toneMeaningContainer);
                    meaningContainer.appendChild(toneSection);
                });
            } else {
                // 如果只有單一讀音
                const meaningSources = Object.keys(characterData.meaning);
                meaningSources.forEach(source => {
                    const rowDiv = document.createElement('div');
                    rowDiv.className = 'grid grid-cols-[120px_1fr] gap-x-2 py-1';
                    const sourceDiv = document.createElement('div');
                    sourceDiv.textContent = source;
                    rowDiv.appendChild(sourceDiv);
                    const meaningDiv = document.createElement('div');
                    // 處理漢語大字典的格式
                    if (source === '漢語大字典') {
                        const meanings = characterData.meaning[source].split(/\d\.\s*/).filter(m => m.trim() !== '');
                        const list = document.createElement('ul');
                        list.className = 'list-none p-0 m-0';
                        meanings.forEach((meaning, index) => {
                            const listItem = document.createElement('li');
                            listItem.textContent = `${index + 1}. ${meaning.trim()}`;
                            list.appendChild(listItem);
                        });
                        meaningDiv.appendChild(list);
                    } else {
                        meaningDiv.textContent = characterData.meaning[source];
                    }
                    rowDiv.appendChild(meaningDiv);
                    meaningContainer.appendChild(rowDiv);
                });
            }
            meaningSection.appendChild(meaningContainer);
            characterCard.appendChild(meaningSection);
            return characterCard;
        }

        /**
         * 函式：利用 canvas 生成中文字符的圖片。
         * @param {string} char - 要渲染的字符。
         * @param {number} size - 字體大小（像素）。
         * @returns {string} 生成的圖片的 Data URL。
         */
        function generateCharacterImage(char, size) {
            const canvas = document.createElement('canvas');
            const ctx = canvas.getContext('2d');
            canvas.width = size;
            canvas.height = size;
            ctx.fillStyle = '#fff';
            ctx.fillRect(0, 0, canvas.width, canvas.height);
            ctx.fillStyle = '#000';
            ctx.font = `${size}px "DFKai-SB", "BiauKai", "Source Han Sans TC"`;
            ctx.textAlign = 'center';
            ctx.textBaseline = 'middle';
            ctx.fillText(char, canvas.width / 2, canvas.height / 2);
            return canvas.toDataURL('image/png');
        }

        /**
         * 函式：根據使用者輸入的查詢詞渲染結果。
         * @param {string} query 使用者輸入的字串。
         */
        function searchDictionary(query) {
            const trimmedQuery = query.trim();
            const lowerCaseQuery = trimmedQuery.toLowerCase();
            resultContainer.innerHTML = ''; // 清空舊的結果
            
            if (!trimmedQuery) {
                resultContainer.innerHTML = `<p class="text-gray-500 text-lg text-center">在這裡輸入一個字來查詢。</p>`;
                return;
            }

            const dictionaryToSearch = window.defaultDictionaryData;
            
            // 檢查是否有精確匹配的字
            const exactMatchData = dictionaryToSearch[trimmedQuery];
            if (exactMatchData) {
                const card = createCharacterCard(exactMatchData);
                resultContainer.appendChild(card);
                return;
            }
            
            // 如果沒有精確匹配，則進行模糊搜尋
            const allCharacters = Object.values(dictionaryToSearch);
            const results = allCharacters.filter(item => {
                const pinyinMatch = item.tones 
                    ? item.tones.some(t => t.pinyin.includes(lowerCaseQuery)) 
                    : (item.pinyin && item.pinyin.includes(lowerCaseQuery));
                
                const zhuyinMatch = item.tones 
                    ? item.tones.some(t => t.zhuyin.includes(trimmedQuery))
                    : (item.zhuyin && item.zhuyin.includes(trimmedQuery));
                
                const radicalMatch = item.radical && item.radical.includes(trimmedQuery);

                // 新增的字形描述搜尋邏輯
                const descriptionMatch = item.description && item.description.includes(trimmedQuery);

                return pinyinMatch || zhuyinMatch || radicalMatch || descriptionMatch;
            });

            // 渲染模糊搜尋結果
            if (results.length > 0) {
                const listTitle = document.createElement('h2');
                listTitle.className = 'text-2xl text-gray-800 mb-4 text-center';
                listTitle.textContent = `找到 ${results.length} 個相關結果：`;
                resultContainer.appendChild(listTitle);

                const resultsGrid = document.createElement('div');
                resultsGrid.className = 'grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-4';
                results.forEach(item => {
                    const charButton = document.createElement('button');
                    charButton.className = 'results-grid-button bg-blue-100 text-blue-800 rounded-lg hover:bg-blue-200 transition-colors';
                    
                    const charText = document.createElement('span');
                    charText.textContent = item.char;
                    charText.className = `text-base ${item.char === '󸾯' ? 'font-zhengkai-unicode2' : ''}`;
                    
                    charButton.appendChild(charText);
                    
                    // 點擊按鈕時，將該字填入輸入框並重新搜尋
                    charButton.addEventListener('click', () => {
                        searchInput.value = item.char;
                        searchDictionary(item.char);
                    });
                    resultsGrid.appendChild(charButton);
                });
                resultContainer.appendChild(resultsGrid);
            } else {
                // 沒有找到結果
                resultContainer.innerHTML = `<p class="text-gray-500 text-lg text-center">找不到「${trimmedQuery}」這個字或詞。請試試其他字。</p>`;
            }
        }
        
        /**
         * 函式：切換顯示的頁面
         * @param {string} pageName - 'dictionary' 或 'info'
         */
        function showPage(pageName) {
            const allNavButtons = document.querySelectorAll('.nav-button');
            allNavButtons.forEach(btn => btn.classList.remove('bg-gray-400', 'text-white'));
            
            if (pageName === 'dictionary') {
                dictionaryView.classList.remove('hidden');
                infoView.classList.add('hidden');
                navDictionaryButton.classList.add('bg-gray-400', 'text-white');
            } else if (pageName === 'info') {
                dictionaryView.classList.add('hidden');
                infoView.classList.remove('hidden');
                navInfoButton.classList.add('bg-gray-400', 'text-white');
            }
        }

        // =========================================================================
        // 事件監聽器
        // =========================================================================
        
        // 監聽導航按鈕點擊事件
        navInfoButton.addEventListener('click', () => showPage('info'));
        navDictionaryButton.addEventListener('click', () => showPage('dictionary'));

        // 監聽搜尋按鈕點擊事件
        searchButton.addEventListener('click', () => {
            searchDictionary(searchInput.value);
        });

        // 監聽輸入框的 Enter 鍵
        searchInput.addEventListener('keyup', (event) => {
            if (event.key === 'Enter') {
                searchDictionary(searchInput.value);
            }
        });

        // 初始化應用程式
        initializeAppAndAuth().then(() => {
            showPage('info'); // 預設顯示編輯說明頁面
        });
    </script>
</body>
</html>
?
