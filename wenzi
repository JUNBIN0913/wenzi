    <script type="module">
        // =========================================================================
        // JavaScript 程式碼
        // =========================================================================

        // 導入 Firebase SDK 模組
        import { initializeApp } from "https://www.gstatic.com/firebasejs/11.6.1/firebase-app.js";
        import { getAuth, signInWithCustomToken, signInAnonymously } from "https://www.gstatic.com/firebasejs/11.6.1/firebase-auth.js";
        import { getFirestore } from "https://www.gstatic.com/firebasejs/11.6.1/firebase-firestore.js";

        // 全域變數，從 Canvas 環境中取得
        const appId = typeof __app_id !== 'undefined' ? __app_id : 'default-app-id';
        const firebaseConfig = JSON.parse(typeof __firebase_config !== 'undefined' ? __firebase_config : '{}');
        const initialAuthToken = typeof __initial_auth_token !== 'undefined' ? __initial_auth_token : null;

        let db, auth, userId;

        // 應用程式初始化
        async function initializeAppAndAuth() {
            try {
                // 檢查 firebaseConfig 是否為空物件
                if (Object.keys(firebaseConfig).length === 0) {
                    console.error("Firebase config is missing. App cannot be initialized.");
                    return;
                }
                const app = initializeApp(firebaseConfig);
                auth = getAuth(app);
                db = getFirestore(app);

                if (initialAuthToken) {
                    await signInWithCustomToken(auth, initialAuthToken);
                } else {
                    await signInAnonymously(auth);
                }
                userId = auth.currentUser.uid;
                console.log("Firebase initialized successfully. User ID:", userId);

            } catch (error) {
                console.error("Error initializing Firebase:", error);
            }
        }
        
        // 取得 HTML 元素
        const searchInput = document.getElementById('searchInput');
        const searchButton = document.getElementById('searchButton');
        const resultContainer = document.getElementById('resultContainer');
        const navInfoButton = document.getElementById('nav-info');
        const navDictionaryButton = document.getElementById('nav-dictionary');
        const infoView = document.getElementById('info-view');
        const dictionaryView = document.getElementById('dictionary-view');


        // 定義一個全域物件來儲存原始字典資料
        window.defaultDictionaryData = {
            "美": { char: "美", description: "⿱羊大", pinyin: "měi", zhuyin: "ㄇㄟˇ", radical: "羊", strokes: 9, meaning: { "說文解字": "甘也。从羊大。","廣韻": "莫軌切，上聲，旨韻。好也、善也。", "中華字海": "美，古作羊大。本義是味美，引申為好、善。後又引申為華麗、漂亮。", "漢語大字典": "1. 好，善。2. 貌、姿態漂亮。3. 喜歡，稱讚。4. 使...變好、變美。5. 美洲的簡稱。", "其他": "形容人或事物外表、內在、或行為上的優點。" } },
            "家": { char: "家", description: "⿱宀豕", pinyin: "jiā", zhuyin: "ㄐㄧㄚ", radical: "宀", strokes: 10, meaning: { "說文解字": "居也。从宀，豭省聲。", "廣韻": "古牙切，平聲，麻韻。居也。", "中華字海": "家，本義指人們共同生活的場所，引申為家庭、住處。", "漢語大字典": "1. 共同生活的眷屬。2. 居住的地方.3. 學術或藝術流派.4. 職業家。", "其他": "指家庭或住處。" } },
            "愛": { char: "愛", description: "⿳𠬪心夊", pinyin: "ài", zhuyin: "ㄞˋ", radical: "心", strokes: 13, meaning: { "說文解字": "行皃（音貌）。徐鉉曰：今俗作愛字。", "廣韻": "烏代切，去聲，怪韻。惠也，惜也。",  "中華字海": "愛，對人或事物有很深的感情，樂於幫助。", "漢語大字典": "1. 喜歡。2. 憐惜，愛護.3. 珍惜。", "其他": "對人或事物有深厚的情感，亦作喜歡或憐惜。" } },
